{
  "scripts": {
    "start": "npm-run-all --parallel css eleventy browsersync",
    "eleventy": "cross-env NODE_ENV=development eleventy --watch",
    "debug": "set DEBUG=* & eleventy",
    "css": "postcss src/static/css/tailwind.css --o _site/css/style.css --watch",
    "build": "cross-env NODE_ENV=production eleventy && cross-env NODE_ENV=production tailwindcss -i src/static/css/tailwind.css -o _site/css/style.css",
    "browsersync": "browser-sync start --server _site --files _site --port 8080 --no-notify --no-open",
    "algolia": "node ./scripts/algolia.js"
  },
  "devDependencies": {
    "@11ty/eleventy": "3.0.0",
    "@playwright/test": "1.49.1",
    "@tailwindcss/typography": "0.5.15",
    "@types/node": "22.10.5",
    "algolia-indexing": "2.0.0",
    "algoliasearch": "5.18.0",
    "alpinejs": "3.14.8",
    "autoprefixer": "10.4.20",
    "browser-sync": "3.0.3",
    "cross-env": "7.0.3",
    "cssnano": "5.1.15",
    "flowbite": "2.5.2",
    "instantsearch.js": "4.75.7",
    "js-yaml": "4.1.0",
    "luxon": "2.5.2",
    "markdown-it-anchor": "9.2.0",
    "markdown-it-attrs": "4.3.1",
    "markdown-it-table-of-contents": "0.8.0",
    "npm-run-all2": "5.0.2",
    "postcss": "8.4.49",
    "postcss-cli": "9.1.0",
    "tailwindcss": "3.4.17"
  }
}
